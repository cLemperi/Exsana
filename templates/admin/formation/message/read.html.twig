					<div class="custom_title"><h5 class="mb-0">VILLE</h5></div>
						<ul>
							<li>{{ message.UserMessage.city }}</li>
						</ul>
				</div>
			</div>
		</div>
		<div class="col-md-6">
			<div class="card flex-md-row mb-4 box-shadow h-md-250">
				<div class="card-body d-flex flex-column align-items-start">
						<div class="custom_title"><h5 class="mb-0">REQUETE</h5></div>
					<ul>
						<li> {{ message.title }}</li>
					</ul>
				</div>
			</div>
		</div>
