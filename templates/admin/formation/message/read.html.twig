    <div class="row mb-2">
		<div class="col-md-12">
			<div class="card flex-md-row mb-4 box-shadow h-md-250">
				<div class="card-body d-flex flex-column align-items-start">
					<div class="custom_title"><h5 class="mb-0">MESSAGE</h5></div>
						<ul>
							<li>{{ message.content }}</li>
						</ul>
				</div>
			</div>
		</div>
	</div>
